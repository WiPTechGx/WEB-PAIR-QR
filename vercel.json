{
    "version": 2,
    "functions": {
        "api/*.js": {
            "runtime": "@vercel/node@3.0.0",
            "maxDuration": 60
        }
    },
    "rewrites": [
        {
            "source": "/code",
            "destination": "/api/code"
        }
    ],
    "buildCommand": "chmod +x deploy-functions.sh && ./deploy-functions.sh || true",
    "outputDirectory": "public",
    "framework": null,
    "cleanUrls": true
}